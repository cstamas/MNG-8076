<settings>
  <localRepository>${user.dir}/local-repo</localRepository>
  <profiles>
    <profile>
      <id>oss-development</id>
      <repositories>
        <repository>
          <id>something</id>
          <url>https://repo.maven.apache.org/maven2</url>
